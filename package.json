{
  "name": "purescript-mini-redux",
  "version": "1.0.0",
  "description": "An idiomatic mini-interface to Redux for PureScript",
  "scripts": {
    "build": "npm run build:ps",
    "build:ps": "pulp build",
    "build:json": "pulp build --include test --no-psa --json-errors",
    "postinstall": "bower install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ecliptic/purescript-mini-redux.git"
  },
  "author": "Brandon Konkle <brandon@ecliptic.io>",
  "license": "MIT",
  "keywords": [
    "purescript",
    "react",
    "redux"
  ],
  "dependencies": {
    "react": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "pscid": "^1.10.0",
    "pulp": "^9.0.1",
    "purescript": "^0.9.2",
    "purescript-psa": "^0.3.9",
    "standard": "^8.0.0"
  }
}
