{
  "name": "purescript-mini-redux",
  "version": "1.1.0",
  "description": "An idiomatic mini-interface to Redux for PureScript",
  "scripts": {
    "build": "npm run build:ps",
    "build:ps": "pulp build",
    "build:json": "pulp build --include test --no-psa --json-errors",
    "postinstall": "bower install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ecliptic/purescript-mini-redux.git"
  },
  "author": "Brandon Konkle <brandon@ecliptic.io>",
  "license": "MIT",
  "keywords": [
    "purescript",
    "react",
    "redux"
  ],
  "dependencies": {
    "react": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "pscid": "^1.12.1",
    "pulp": "^10.0.1",
    "purescript": "^0.10.7",
    "purescript-psa": "^0.4.0",
    "standard": "^9.0.0"
  }
}
